<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPick</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="MyTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:textViewStyle">@style/NetmarbleLightTextViewStyle</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="NetmarbleLightTextViewStyle" parent="Widget.AppCompat.TextView">
        <item name="fontFamily">@font/netmarblel</item>
    </style>

</resources>