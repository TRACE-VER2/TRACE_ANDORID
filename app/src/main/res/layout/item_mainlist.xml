<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#ffffff"
    android:layout_height="123dp">

    <ImageView
        android:id="@+id/item_house_example"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:src="@drawable/house_example"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/item_tv_adress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="25dp"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/netmarblem"
        android:text="성북구 성북동1가 60-1 범진빌리지"
        android:textColor="#070000"
        android:textSize="16dp"
        app:layout_constraintStart_toEndOf="@+id/item_house_example"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_star_1st"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:src="@drawable/star_yes"
        app:layout_constraintStart_toStartOf="@+id/item_tv_adress"
        app:layout_constraintTop_toBottomOf="@+id/item_tv_adress" />

    <ImageView
        android:id="@+id/img_star_2nd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:src="@drawable/star_yes"
        app:layout_constraintStart_toEndOf="@+id/img_star_1st"
        app:layout_constraintTop_toTopOf="@+id/img_star_1st" />

    <ImageView
        android:id="@+id/img_star_3rd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:src="@drawable/star_yes"
        app:layout_constraintStart_toEndOf="@+id/img_star_2nd"
        app:layout_constraintTop_toTopOf="@+id/img_star_2nd" />

    <ImageView
        android:id="@+id/img_star_4th"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:src="@drawable/star_yes"
        app:layout_constraintStart_toEndOf="@+id/img_star_3rd"
        app:layout_constraintTop_toTopOf="@+id/img_star_3rd" />

    <ImageView
        android:id="@+id/img_star_5th"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:src="@drawable/star_blank"
        app:layout_constraintStart_toEndOf="@+id/img_star_4th"
        app:layout_constraintTop_toTopOf="@+id/img_star_4th" />

</androidx.constraintlayout.widget.ConstraintLayout>