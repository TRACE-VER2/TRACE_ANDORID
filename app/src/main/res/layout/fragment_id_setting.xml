<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg_grad_3"
    tools:context=".startpage.KingoFirstFragment">

    <ImageView
        android:id="@+id/iv_user_check_logo"
        android:layout_width="200dp"
        android:layout_height="100dp"
        android:layout_marginTop="63dp"
        android:src="@drawable/trace_new_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="285dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:src="@drawable/tv_set_id"
        app:layout_constraintEnd_toEndOf="@+id/iv_user_check_logo"
        app:layout_constraintStart_toStartOf="@+id/iv_user_check_logo"
        app:layout_constraintTop_toBottomOf="@+id/iv_user_check_logo" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="38dp"
        android:hint="아이디"
        android:lineSpacingExtra="5sp"
        android:textColor="@color/colorDarkGray"
        android:textSize="17sp"
        android:textStyle="normal"
        app:layout_constraintStart_toStartOf="@+id/editText2"
        app:layout_constraintTop_toBottomOf="@+id/imageView4" />

    <ImageView
        android:id="@+id/signup_btn_check"
        android:layout_width="70dp"
        android:layout_height="35dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:src="@drawable/signup_iv_check"
        app:layout_constraintBottom_toBottomOf="@+id/editText"
        app:layout_constraintStart_toEndOf="@+id/editText"
        app:layout_constraintTop_toTopOf="@+id/editText" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="중복확인"
        android:fontFamily="@font/netmarblem"
        android:textColor="#4a4949"
        android:textSize="17dp"
        app:layout_constraintBottom_toBottomOf="@+id/signup_btn_check"
        app:layout_constraintEnd_toEndOf="@+id/signup_btn_check"
        app:layout_constraintStart_toStartOf="@+id/signup_btn_check"
        app:layout_constraintTop_toTopOf="@+id/signup_btn_check" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="285dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:hint="비밀번호"
        android:lineSpacingExtra="5sp"
        android:textColor="@color/colorDarkGray"
        android:textSize="17sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imageView4"
        app:layout_constraintStart_toStartOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/editText" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="285dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:hint="비밀번호 확인"
        android:lineSpacingExtra="5sp"
        android:textColor="@color/colorDarkGray"
        android:textSize="17sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/editText2"
        app:layout_constraintStart_toStartOf="@+id/editText2"
        app:layout_constraintTop_toBottomOf="@+id/editText2" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:src="@drawable/btn_agree_unpick"
        app:layout_constraintEnd_toEndOf="@+id/editText3"
        app:layout_constraintStart_toStartOf="@+id/editText3"
        app:layout_constraintTop_toBottomOf="@+id/editText3" />

    <ImageView
        android:id="@+id/signup_btn_last"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:src="@drawable/btn_signup_last"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/netmarblem"
        android:lineSpacingExtra="10dp"
        android:text="회 원 가 입"
        android:textColor="#494747"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/signup_btn_last"
        app:layout_constraintEnd_toEndOf="@+id/signup_btn_last"
        app:layout_constraintStart_toStartOf="@+id/signup_btn_last"
        app:layout_constraintTop_toTopOf="@+id/signup_btn_last" />



</androidx.constraintlayout.widget.ConstraintLayout>